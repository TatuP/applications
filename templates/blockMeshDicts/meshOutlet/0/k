FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      k;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 15000;

boundaryField
{
    cooler
    {
        type            fixedValue;
        value           $internalField;
    }
    fuelAir
    {
        type            fixedValue;
        value           $internalField;
    }
    carrier
    {
        type            fixedValue;
        value           $internalField;
    }
    walls
    {
        type            compressible::kqRWallFunction;
        value           $internalField;
    }
    midWalls
    {
        type            slip;
    }
    symmetric
    {
        type            symmetryPlane;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack_pos
    {
        type            wedge;
    }
    frontAndBack_neg
    {
        type            wedge;
    }
    divOutlet
    {
	    type slip;
    }
    outsideInlet
    {
	    type slip;
    }
}


// ************************************************************************* //
