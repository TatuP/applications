FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    cooler
    {
        type            fixedValue;
        value           uniform 0;
    }
    fuelAir
    {
        type            fixedValue;
        value           uniform 0;
    }
    carrier
    {
        type            fixedValue;
        value           uniform 0;
    }
    walls
    {
        type            alphatWallFunction;
        Prt             0.85;
        value           uniform 0;
    }
    midWalls
    {
	type		slip;
    }
    symmetric
    {
        type            symmetryPlane;
    }
    frontAndBack
    {
        type            empty;
    }
    outlet
    {
        type            zeroGradient;
    }
    frontAndBack_pos
    {
        type            wedge;
    }
    frontAndBack_neg
    {
        type            wedge;
    }
    divOutlet
    {
	    type slip;
    }
    outsideInlet
    {
	    type slip;
    }
}


